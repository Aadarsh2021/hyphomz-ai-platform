{
  "name": "hyphomz-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend:dev": "cd backend && python main.py",
    "backend:install": "cd backend && pip install -r requirements.txt",
